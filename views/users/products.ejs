															<img src="/products/croped/<%= product && product.images && product.images.image1 %>" alt="Product image" class="product-image">
														</a>
									
														<div class="product-action-vertical">
															<p onclick="addToWishlist('<%= product._id %>')" class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></p>
														</div><!-- End .product-action-vertical -->
									
														<div class="product-action">
															<p onclick="addToCart('<%= product._id %>')" class="btn-product btn-cart"><span>add to cart</span></p>
														</div><!-- End .product-action -->
													</figure><!-- End .product-media -->
									
													<div class="product-body">
